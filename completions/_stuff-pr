#compdef stuff-pr

_stuff-pr() {
  local -a dirs
  local cur context state line

  _arguments -C \
    '*:dirs:->dirs'

  case $state in
    (dirs)
      dirs=($(find "$PROJECTS_DIR" -maxdepth 1 -type d -not -path '*/\.*' -not -name "$(basename $PROJECTS_DIR)" -printf "%P\n"))
      cur="${COMP_WORDS[COMP_CWORD]}"
      _describe -t dirs "project" dirs
      ;;
  esac
}

compdef _stuff-pr stuff-pr

